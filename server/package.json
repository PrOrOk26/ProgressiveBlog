{
  "name": "progressive-blog",
  "version": "1.0.0",
  "description": "ProgressiveBlog",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -e ts -w ./src --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "author": "prorok26",
  "license": "MIT",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "es5",
    "jsxBracketSameLine": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.6.4",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2"
  }
}
